package domain

class Author(
    @Summary
    val name: string,
    val bio: string?,
    val birthYear: int?
) {
    static val nameIdx = Index<string, Author>(false, a -> a.name)
    static val allIdx = Index<bool, Author>(false, a -> true)
    
    fn getBooks() -> Book[] {
        return Book.authorIdx.getAll(this)
    }
}